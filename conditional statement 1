#include<stdio.h>
main()
{
	float m1,m2,m3,m4,m5,n;
	printf("enter 5 marks :");
	scanf("%f %f %f %f %f",&m1,&m2,&m3,&m4,&m5);
	n=(m1+m2+m3+m4+m5)/5;
	if(n>=85)
	{
		printf("Grade A");
	}
	else if(n>=70)
	{
		printf("Grade B");
	}
	else if(n>=55)
	{
		printf("Grade C");
	} 	
	else if(n>=40)
	{
		printf("Grade D");
	}
	else
	{
		printf("Grade E");
	}
}
